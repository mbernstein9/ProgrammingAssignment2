UL=10
LL=-10
modes=256
N=(UL-LL)/(modes-1)
noise=2.5
center=runif(1)*(UL-LL)+LL
x<-seq(LL,UL,by=N)
y<-dnorm(x-center)
y_fft<-fft(y)
ny_fft_shift<-complex(real=Re(y_fft)+noise*rnorm(length(y_fft)),imaginary=Im(y_fft)+noise*rnorm(length(y_fft)))
half1<-seq(1,modes-1,by=2)
half2<-seq(modes,2,by=-2)
x_shift<-c(half1,half2)
ny_fft<-abs(ny_fft_shift[x_shift])
lo<-matrix(1:2,nrow=2,ncol=1)
layout(lo)
plot(-128:127,ny_fft,type="p",col="blue",xlab="Modes",ylab="Magnitude")
iny_fft<-abs(fft(ny_fft_shift,inverse=TRUE))
